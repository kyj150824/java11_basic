package org.kh.site;
//산술　연산　＝＞　논리　연산　
public class Operation２ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//＋，－，＊，／，％
		int su1 = 98;
		int su2 = 18;
		System.out.println("su1 + su2 =" + (su1 + su2));
		System.out.println("su1 - su2 =" + (su1 - su2));
		System.out.println("su1 * su2 =" + (su1 * su2));
		System.out.println("su1 / su2 =" + (su1 / su2));
		System.out.println("su1 % su2 =" + (su1 % su2)); // 98/18 = 5 ...8
		
		
	}

}
